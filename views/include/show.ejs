









<!-- creating for loop to traverse over all the data of the users -->
<% for (var i=0;i<users.length;i++) {%> 



<tr>
    <td><%= i+1 %> </td>
    <td> <%= users[i].name %> </td>                    <!-- .name .email are case-sensitive (put same as DB)-->
    <td><%= users[i].email %> </td>
    <td><%= users[i].gender %> </td>
    <td><%= users[i].status %> </td>
    <td>
        <a href="/update-user?id=<%= users[i]._id %> " class="btn border-shadow update">
            <span class="text-gradient"><i class="fas fa-pencil-alt"></i>

            </span>
        </a>
        <a  class="btn border-shadow delete" data-id=<%=users[i]._id %> >
            <span class="text-gradient"><i class="fas fa-times"></i>

            </span>
        </a>
    </td>
</tr>


<% } %> 